{
  "name": "Ibux",
  "version": "1.0.0",
  "description": "simple shop management",
  "author": "Muhammad Juan Akbar",
  "scripts": {
    "app:start": "node index.js",
    "depedencies:version": "npm-check",
    "db:migrate": "node_modules\\.bin\\sequelize db:migrate",
    "db:seed": "node_modules\\.bin\\sequelize db:seed:all"
  },
  "dependencies": {
