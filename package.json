    "express": "^4.14.0",
    "lodash": "^4.16.6",
    "moment": "^2.17.1",
    "mysql": "^2.13.0",
    "sequelize": "^3.30.2",
    "uuid": "^3.0.1",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.14.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.1.0",
    "mocha": "^3.1.2",
    "sequelize-cli": "^2.5.1",
    "sinon": "^1.17.6",
    "sinon-mongoose": "^1.3.0",
    "supertest": "^2.0.1"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
